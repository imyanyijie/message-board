<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul>
      <% messages.forEach((message) =>{ %>
        <li>
          <p><%= message.text %></p>
          <p><%= message.user %></p>
          <p><%= message.added %></p>
        </li>
        <% }) %>
    </ul>
    <p>Please Click <a href="/new">Here</a> to create a new message</p>
  </body>
</html>
